server {
    listen 80;
    server_name id_here;

    location = /favicon.ico { access_log off; log_not_found off; }
    location /static/ {
        root /home/ubuntu/service24;
    }

    location / {
        include proxy_params;
        proxy_pass http://unix:/home/ubuntu/service24/service24.sock;
    }
}
